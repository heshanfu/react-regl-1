{
  "name": "react-regl",
  "version": "0.0.1",
  "description": "React fiber renderer and component container for Regl",
  "main": "./index.js",
  "repository": "https://github.com/psychobolt/react-regl.git",
  "author": "psychobolt",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install -s -o --ignoreDeps peer",
    "build:dev": "rollup -c rollup.config.dev.js",
    "build:prod": "rollup -c rollup.config.prod.js",
    "build": "rimraf dist/index.*.* dist/*.css && npm run build:dev && npm run build:prod",
    "start": "npm run build:dev -- -w",
    "test": "cross-env BABEL_ENV=commonjs jest --passWithNoTests",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint ./src/**/*.js",
    "lint:js": "eslint .storybook src stories *.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "setupFiles": [
      "./test-config.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/(index|*.stories).js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.mock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/style.mock.js"
    }
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-console": "^1.0.4",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addon-notes": "^3.4.10",
    "@storybook/react": "^3.4.8",
    "angle-normals": "^1.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "^23.4.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "baboon-image": "^2.1.0",
    "bunny": "^1.0.1",
    "canvas-orbit-camera": "^1.0.2",
    "cash-true": "^0.0.2",
    "codecov": "^3.0.4",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "fbjs": "0.8.17",
    "flow-bin": "^0.76.0",
    "flow-typed": "2.5.1",
    "getusermedia": "^2.0.1",
    "gl-mat4": "^1.2.0",
    "gl-vec2": "^1.2.0",
    "hsv2rgb": "^1.1.0",
    "jest": "^23.4.1",
    "jest-styled-components": "^5.0.1",
    "lodash": "4.17.10",
    "mouse-change": "^1.4.0",
    "prop-types": "15.6.2",
    "raf": "^3.4.0",
    "raf-schd": "^4.0.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-reconciler": "0.12.0",
    "react-resize-detector": "^3.1.1",
    "recompose": "^0.27.1",
    "regl": "^1.3.7",
    "regl-stats-widget": "^0.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-uglify": "^4.0.0",
    "seedrandom": "^2.4.4",
    "standardized-audio-context": "^14.0.1",
    "storybook-readme": "^3.3.0",
    "styled-components": "^3.3.3",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "uglify-es": "^3.3.9",
    "vectorize-text": "^3.0.2",
    "webpack-glsl-loader": "^1.0.1"
  },
  "dependencies": {
    "react-is": "^16.4.1",
    "resl": "^1.0.3"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "regl": "^1.3.7"
  }
}
