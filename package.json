    "prop-types": "^15.6.2",
    "raf": "^3.4.0",
    "raf-schd": "^4.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-reconciler": "0.12.0",
    "react-resize-detector": "^3.1.1",
    "recompose": "^0.30.0",
    "regl": "^1.3.7",
    "regl-stats-widget": "^0.0.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-scss": "^0.4.0",
    "rollup-plugin-uglify": "^6.0.0",
    "seedrandom": "^2.4.4",
    "standardized-audio-context": "^14.0.1",
    "storybook-readme": "^4.0.0-beta1",
    "styled-components": "^3.4.9",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "uglify-es": "^3.3.9",
    "vectorize-text": "^3.0.2",
    "webpack-glsl-loader": "^1.0.1"
  },
  "dependencies": {
    "react-is": "^16.5.2",
    "resl": "^1.0.3"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "regl": "^1.3.7"
  }
}
